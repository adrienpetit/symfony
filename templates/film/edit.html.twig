{% extends 'base.html.twig' %}



{% block body %}


	<h3> Modification d'un Film</h3>
	{{ form_start(form)}}

	

	{{ form_row(form.title,{'attr':{'placeholder':"Titre du film"}}) }}
	{{ form_row(form.category) }}
	{{ form_row(form.content,{'attr':{'placeholder':"Résumé du film"}}) }}
	{{ form_row(form.image,{'attr':{'placeholder':"Url de l'image"}}) }}

	<button type="submit" class="btn-success"> 
		

			Modifier film
			

	</button>
	
		
	{{ form_end(form)}}

{% endblock %}